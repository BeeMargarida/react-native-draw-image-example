{
    "name": "react-native-draw-image-example",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "clear": "react-native start --reset-cache",
        "lint": "eslint .",
        "lint-fix": "eslint \"./**/*.{js,jsx}\" --fix",
        "prettier": "prettier \"./**/*.{js,json}\" --write",
        "pretty": "npm run prettier && npm run lint-fix",
        "wml": "wml start",
        "wml-link-draw-image": "wml add ${REACT_NATIVE_DRAW_IMAGE_PATH-../react-native-draw-image} node_modules/react-native-draw-image"
    },
    "dependencies": {
        "react": "17.0.2",
        "react-native": "0.67.3",
        "react-native-gesture-handler": "^2.3.1",
        "react-native-svg": "^12.3.0",
        "react-native-view-shot": "^3.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "babel-jest": "^26.6.3",
        "eslint": "7.14.0",
        "metro-react-native-babel-preset": "^0.66.2",
        "prettier": "^2.5.1",
        "react-test-renderer": "17.0.2",
        "wml": "^0.0.83"
    },
    "jest": {
        "preset": "react-native"
    }
